 #include<stdio.h>

#include<conio.h>
int returnMax(int array[],int n){
    int max = 0;
    for (int i = 0; i < n; i++)
    {
        /* code */
        if (array[i]>max)
        {
            /* code */
            max = array[i];
        }
        
    }
    return max;
    
}
void main(){
    int arr[] = {5, 8, 6, 15, 7};
    
    int max = returnMax(arr, 5);
    printf("%d", max);

    getch();    
}
